{
  "name": "resist",
  "version": "1.0.0",
  "description": "Web-based version of The Resistance",
  "main": "server.coffee",
  "scripts": {
    "start": "./node_modules/.bin/coffee server.coffee",
    "test": "./node_modules/.bin/mocha --recursive --reporter spec --compilers coffee:coffee-script/register",
    "start-dev": "./node_modules/.bin/supervisor -w src/server,src/assets/coffee/shared.coffee server.coffee",
    "g": "./node_modules/.bin/gulp",
    "gw": "./node_modules/.bin/gulp watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bharley/resist.git"
  },
  "keywords": [
    "game"
  ],
