  "license": "MIT",
  "bugs": {
    "url": "https://github.com/bharley/resist/issues"
  },
  "homepage": "https://github.com/bharley/resist#readme",
  "dependencies": {
    "coffee-script": "^1.9.3",
    "express": "^4.13.1",
    "moniker": "^0.1.2",
    "socket.io": "^1.3.6"
  },
  "devDependencies": {
    "chai": "*",
    "gulp": "^3.9.0",
    "gulp-angular-templatecache": "^1.7.0",
    "gulp-coffee": "^2.3.1",
    "gulp-concat": "^2.6.0",
    "gulp-plumber": "^1.0.1",
    "gulp-rename": "^1.2.2",
    "gulp-sass": "^2.0.4",
    "gulp-sourcemaps": "^1.5.2",
    "gulp-uglify": "^1.2.0",
    "mocha": "*",
